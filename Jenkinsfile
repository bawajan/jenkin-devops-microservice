//DECLARATIVE
pipeline {
	agent any  ## Agent here is like a node where the Build runs -Same "agent" will be referred in Azure PipeLines as well
	stages {
		stage ('Build') {
			steps {
				echo "Build"
			}
		}
		stage ('Test') {
			steps {
				echo "Test"
			}
		}
				stage ('Integration Test') {
			steps {
				echo "Integration Test"
			}
		}
	}
}